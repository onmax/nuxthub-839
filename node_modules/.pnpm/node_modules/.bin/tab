#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@bomb.sh+tab@0.0.12_cac@6.7.14_citty@0.2.1/node_modules/@bomb.sh/tab/dist/bin/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@bomb.sh+tab@0.0.12_cac@6.7.14_citty@0.2.1/node_modules/@bomb.sh/tab/dist/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@bomb.sh+tab@0.0.12_cac@6.7.14_citty@0.2.1/node_modules/@bomb.sh/tab/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@bomb.sh+tab@0.0.12_cac@6.7.14_citty@0.2.1/node_modules/@bomb.sh/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@bomb.sh+tab@0.0.12_cac@6.7.14_citty@0.2.1/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@bomb.sh+tab@0.0.12_cac@6.7.14_citty@0.2.1/node_modules/@bomb.sh/tab/dist/bin/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@bomb.sh+tab@0.0.12_cac@6.7.14_citty@0.2.1/node_modules/@bomb.sh/tab/dist/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@bomb.sh+tab@0.0.12_cac@6.7.14_citty@0.2.1/node_modules/@bomb.sh/tab/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@bomb.sh+tab@0.0.12_cac@6.7.14_citty@0.2.1/node_modules/@bomb.sh/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@bomb.sh+tab@0.0.12_cac@6.7.14_citty@0.2.1/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@bomb.sh/tab/dist/bin/cli.js" "$@"
else
  exec node  "$basedir/../@bomb.sh/tab/dist/bin/cli.js" "$@"
fi

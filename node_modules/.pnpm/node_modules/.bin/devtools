#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@nuxt+devtools@3.2.1_vite@7.3.1_jiti@2.6.1_terser@5.46.0_yaml@2.8.2__vue@3.5.28/node_modules/@nuxt/devtools/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@nuxt+devtools@3.2.1_vite@7.3.1_jiti@2.6.1_terser@5.46.0_yaml@2.8.2__vue@3.5.28/node_modules/@nuxt/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@nuxt+devtools@3.2.1_vite@7.3.1_jiti@2.6.1_terser@5.46.0_yaml@2.8.2__vue@3.5.28/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@nuxt+devtools@3.2.1_vite@7.3.1_jiti@2.6.1_terser@5.46.0_yaml@2.8.2__vue@3.5.28/node_modules/@nuxt/devtools/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@nuxt+devtools@3.2.1_vite@7.3.1_jiti@2.6.1_terser@5.46.0_yaml@2.8.2__vue@3.5.28/node_modules/@nuxt/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/@nuxt+devtools@3.2.1_vite@7.3.1_jiti@2.6.1_terser@5.46.0_yaml@2.8.2__vue@3.5.28/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nuxt/devtools/cli.mjs" "$@"
else
  exec node  "$basedir/../@nuxt/devtools/cli.mjs" "$@"
fi

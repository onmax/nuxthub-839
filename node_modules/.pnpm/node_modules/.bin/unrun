#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/unrun@0.2.27/node_modules/unrun/dist/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/unrun@0.2.27/node_modules/unrun/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/unrun@0.2.27/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/unrun@0.2.27/node_modules/unrun/dist/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/unrun@0.2.27/node_modules/unrun/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/unrun@0.2.27/node_modules:/Users/maxi/repros/nuxthub-839/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../unrun/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../unrun/dist/cli.mjs" "$@"
fi
